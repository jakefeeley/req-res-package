{
  "name": "jakefeeley/req-res-package",
  "description": "A PHP framework agnostic package for interacting with the ReqRes API",
  "type": "library",
  "license": "MIT License",
  "autoload": {
    "psr-4": {
      "Jakefeeley\\ReqResPackage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jakefeeley\\ReqResPackage\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jake Feeley",
      "email": "jacob.feeley@protonmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  }
}
